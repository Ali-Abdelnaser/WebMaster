{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "www.ieeemet.org" }
      ],
      "destination": "https://ieeemet.org/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "scheme", "value": "http" }
      ],
      "destination": "https://ieeemet.org/$1",
      "permanent": true
    }
  ]
}
