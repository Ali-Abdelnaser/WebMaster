{
  "redirects": [
    {
      "source": "/",
      "destination": "https://ieeemet.org",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.ieeemet.org"
        }
      ],
      "destination": "https://ieeemet.org/$1",
      "permanent": true
    }
  ]
}
